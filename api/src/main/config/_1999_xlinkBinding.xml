<binding xmlns:xlink="http://www.w3.org/1999/xlink" trim-whitespace="true">
  <namespace uri="http://www.w3.org/1999/xlink" prefix="xlink"/>
  <mapping abstract="true" type-name="xlink:simpleLink-AttributeGroup" class="org.geomajas.sld.xlink.SimpleLinkInfo">
    <value style="attribute" name="type" ns="http://www.w3.org/1999/xlink" get-method="getType" set-method="setType" usage="optional"/>
    <structure get-method="getHref" set-method="setHref" usage="optional">
      <value style="attribute" name="href" ns="http://www.w3.org/1999/xlink" get-method="getHref" set-method="setHref" usage="optional"/>
    </structure>
    <structure get-method="getRole" set-method="setRole" usage="optional">
      <value style="attribute" name="role" ns="http://www.w3.org/1999/xlink" get-method="getRole" set-method="setRole" usage="optional"/>
    </structure>
    <structure get-method="getArcrole" set-method="setArcrole" usage="optional">
      <value style="attribute" name="arcrole" ns="http://www.w3.org/1999/xlink" get-method="getArcrole" set-method="setArcrole" usage="optional"/>
    </structure>
    <structure get-method="getTitle" set-method="setTitle" usage="optional">
      <value style="attribute" name="title" ns="http://www.w3.org/1999/xlink" get-method="getTitle" set-method="setTitle" usage="optional"/>
    </structure>
    <value style="attribute" name="show" ns="http://www.w3.org/1999/xlink" enum-value-method="xmlValue" get-method="getShow" set-method="setShow" usage="optional"/>
    <value style="attribute" name="actuate" ns="http://www.w3.org/1999/xlink" enum-value-method="xmlValue" get-method="getActuate" set-method="setActuate" usage="optional"/>
  </mapping>
  <mapping abstract="true" type-name="xlink:extendedLink-AttributeGroup" class="org.geomajas.sld.xlink.ExtendedLinkInfo">
    <value style="attribute" name="type" ns="http://www.w3.org/1999/xlink" get-method="getType" set-method="setType" usage="optional"/>
    <structure get-method="getRole" set-method="setRole" usage="optional">
      <value style="attribute" name="role" ns="http://www.w3.org/1999/xlink" get-method="getRole" set-method="setRole" usage="optional"/>
    </structure>
    <structure get-method="getTitle" set-method="setTitle" usage="optional">
      <value style="attribute" name="title" ns="http://www.w3.org/1999/xlink" get-method="getTitle" set-method="setTitle" usage="optional"/>
    </structure>
  </mapping>
  <mapping abstract="true" type-name="xlink:locatorLink-AttributeGroup" class="org.geomajas.sld.xlink.LocatorLinkInfo">
    <value style="attribute" name="type" ns="http://www.w3.org/1999/xlink" get-method="getType" set-method="setType" usage="optional"/>
    <value style="attribute" name="href" ns="http://www.w3.org/1999/xlink" get-method="getHref" set-method="setHref" usage="optional"/>
    <structure get-method="getRole" set-method="setRole" usage="optional">
      <value style="attribute" name="role" ns="http://www.w3.org/1999/xlink" get-method="getRole" set-method="setRole" usage="optional"/>
    </structure>
    <structure get-method="getTitle" set-method="setTitle" usage="optional">
      <value style="attribute" name="title" ns="http://www.w3.org/1999/xlink" get-method="getTitle" set-method="setTitle" usage="optional"/>
    </structure>
    <structure get-method="getLabel" set-method="setLabel" usage="optional">
      <value style="attribute" name="label" ns="http://www.w3.org/1999/xlink" get-method="getLabel" set-method="setLabel" usage="optional"/>
    </structure>
  </mapping>
  <mapping abstract="true" type-name="xlink:arcLink-AttributeGroup" class="org.geomajas.sld.xlink.ArcLinkInfo">
    <value style="attribute" name="type" ns="http://www.w3.org/1999/xlink" get-method="getType" set-method="setType" usage="optional"/>
    <structure get-method="getArcrole" set-method="setArcrole" usage="optional">
